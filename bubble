#include <stdio.h>
#include <stdlib.h>

#define true  1
#define false 0
#define N 10

int main()
{
    int i=0;
    int A[N], temp;

    for (i=0; i<N; i++)
        A[i] = i*i;
        
    while (1)
    {
        if (i > N)
            break;
            
        if (A[i] > A[i+1])
        {
            temp = A[i];
            A[i] = A[i+1];
            A[i+1] = temp;
        }
        i++;
    }
    
    for (i=0; i<N; i++)
        printf("%d \n", A[i]);
        
    return 0;
}
